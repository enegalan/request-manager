{
  "name": "@enegalan/request-manager",
  "version": "1.0.2",
  "description": "JavaScript library for managing and regulating HTTP requests efficiently",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/request-manager.cjs.js",
  "module": "dist/request-manager.esm.js",
  "browser": "dist/request-manager.min.js",
  "unpkg": "dist/request-manager.min.js",
  "jsdelivr": "dist/request-manager.min.js",
  "types": "index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "index.js",
    "main.js",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/request-manager.esm.js",
      "require": "./dist/request-manager.cjs.js",
      "browser": "./dist/request-manager.min.js",
      "default": "./dist/request-manager.esm.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "jest": "^29.7.0",
    "rollup": "^4.28.0"
  },
  "keywords": [
    "http",
    "request",
    "manager",
    "ajax",
    "axios",
    "fetch",
    "cancellation",
    "abort",
    "AbortController",
    "promise",
    "async",
    "xhr",
    "XMLHttpRequest"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enegalan/request-manager.git"
  },
  "bugs": {
    "url": "https://github.com/enegalan/request-manager/issues"
  },
  "homepage": "https://github.com/enegalan/request-manager#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
